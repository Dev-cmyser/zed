{
  "chat_panel": {
    "dock": "right"
  },
  "ssh_connections": [
    {
      "host": "87.120.36.150",
      "username": "root",
      "projects": [
        {
          "paths": ["/home"]
        }
      ],
      "nickname": "my vpn"
    }
  ],
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2",
    "default_open_ai_model": "gpt-4",
    "provider": {
      "name": "openai",
      "default_model": "gpt-4",
      "available_models": []
    }
  },
  "theme": {
    "mode": "light",
    "light": "One Light",
    "dark": "One Dark"
  },

  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "confirm_quit": true,
  "current_line_highlight": "gutter",
  "autosave": "on_window_change",
  "relative_line_numbers": true,
  "preferred_line_length": 120,

  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "delay_ms": 1000
    }
  },

  "project_panel": {
    "auto_fold_dirs": false
  },

  "languages": {
    "Go": {
      "show_inline_completions": true,
      "format_on_save": "on",
      "code_actions_on_format": {
        "source.organizeImports": true,
        "sousce.fixAll": true
      }
    },
    "TypeScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "formatter": "prettier"
    },
    "YAML": {
      "prettier": {
        "bracketSpacing": false,
        "allowed": false
      }
    }
  },
  "terminal": {
    "max_scroll_history_lines": 100000,
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "toolbar": {
    "quick_actions": false,
    "breadcrumbs": false
  },
  "file_types": {
    "Shell Script": [".env.*", "*.env"],
    "Dockerfile": ["*Dockerfile.*"]
  }
}
