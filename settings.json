{
  "edit_predictions": {
    "disabled_globs": ["*.env"],
    "mode": "subtle"
  },
  "chat_panel": {
    "dock": "right"
  },
  "ssh_connections": [
    {
      "host": "87.120.36.150",
      "username": "root",
      "projects": [
        {
          "paths": ["/home"]
        }
      ],
      "nickname": "my vpn"
    }
  ],
  "buffer_font_features": {
    "calt": true
  },
  "ui_font_features": {
    "calt": true
  },
  "features": {
    "edit_prediction_provider": "supermaven"
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2",
    "default_open_ai_model": "gpt-4",
    "provider": {
      "name": "openai",
      "default_model": "gpt-4",
      "available_models": []
    }
  },
  "theme": {
    "mode": "light",
    "light": "One Light",
    "dark": "One Dark"
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "confirm_quit": true,
  "current_line_highlight": "gutter",
  "autosave": "on_window_change",
  "relative_line_numbers": false,
  "preferred_line_length": 120,
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "delay_ms": 1000
    }
  },
  "project_panel": {
    "auto_fold_dirs": false
  },
  "languages": {
    "JSON": {
      "show_edit_predictions": false
    },
    "Go": {
      "show_edit_predictions": true,
      "format_on_save": "on",
      "code_actions_on_format": {
        "source.organizeImports": true,
        "sousce.fixAll": true
      }
    },
    "TypeScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "formatter": "prettier"
    },
    "YAML": {
      "prettier": {
        "allowed": false
      },
      "format_on_save": "off"
    },
    "Proto": {
      "prettier": {
        "allowed": true,
        "plugins": ["@prettier/plugin-proto"]
      },
      "format_on_save": "on"
    },
    "View Tree ($mol)": {
      "language_servers": ["typescript-language-server"],
      "enable_language_server": true
    }
  },
  "terminal": {
    "max_scroll_history_lines": 100000,
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "toolbar": {
    "quick_actions": false,
    "breadcrumbs": false
  },
  "file_types": {
    "Shell Script": [".env.*", "*.env"],
    "Dockerfile": ["*Dockerfile.*"],
    "ini": ["*.web.ts", "*.test.ts", "*.node.ts"]
  },
  "language_models": {
    "custom_llm": {
      "api_url": "https://corellm-retrieval.wb.ru/llama3/v1/chat/completions",
      "headers": {
        "Authorization": "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJPemdJNEFaZVZ4RGw4VWpiSVBDdkpnS0daNFF3TzVueCJ9.WB0NYuBkXuTa7quL_TR2gUVYvs0q6g2WVJpQTGZCVQE",
        "Content-Type": "application/json"
      },
      "available_models": [
        {
          "name": "Meta-Llama-3-70B-Instruct-GPTQ",
          "display_name": "Llama 3 (70B)",
          "max_tokens": 100000
        }
      ]
    }
  },
  "lsp": {
    "protobuf-language-server": {
      "binary": {
        "path": "protols"
      }
    }
  }
}
